<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="KassaView"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  RowClass="ua.divas.view.KassaViewRowImpl">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Access"/>
    <Attr Name="_isCodegen" Value="true"/>
  </DesignTime>
  <ViewAccessor
    Name="CurrencyView1"
    ViewObjectName="ua.divas.view.CurrencyView"
    RowLevelBinds="true"/>
  <ViewAccessor
    Name="DivisionsView1"
    ViewObjectName="ua.divas.view.DivisionsView"
    RowLevelBinds="true"/>
  <ListBinding
    Name="LOV_CurrId"
    ListVOName="CurrencyView1"
    ListRangeSize="-1"
    NullValueFlag="none"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="CurrId"/>
    </AttrArray>
    <AttrArray Name="AttrExpressions"/>
    <AttrArray Name="ListAttrNames">
      <Item Value="Id"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Fullname"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <ListBinding
    Name="LOV_DivisionId"
    ListVOName="DivisionsView1"
    ListRangeSize="-1"
    NullValueFlag="start"
    NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
    MRUCount="0">
    <AttrArray Name="AttrNames">
      <Item Value="DivisionId"/>
    </AttrArray>
    <AttrArray Name="AttrExpressions"/>
    <AttrArray Name="ListAttrNames">
      <Item Value="Id"/>
    </AttrArray>
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="Fullname"/>
    </AttrArray>
    <DisplayCriteria/>
  </ListBinding>
  <SQLQuery><![CDATA[SELECT Kassa.ID, 
       Kassa.FULLNAME, 
       Kassa.IS_GROUP, 
       Kassa.PARENT_ID, 
       Kassa.CURR_ID, 
       Kassa.DIVISION_ID, 
       Kassa.FIRMA_ID, 
       Kassa.DELETED, 
       Kassa.VERSION, 
       Kassa.PREDEFINED,
       (select 0 from dual) k_editable
FROM  KASSA Kassa
where deleted = 0]]></SQLQuery>
  <EntityUsage
    Name="Kassa"
    Entity="ua.divas.model.Kassa"/>
  <ViewAttribute
    Name="Id"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="Kassa"
    AliasName="ID"/>
  <ViewAttribute
    Name="Fullname"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Fullname"
    EntityUsage="Kassa"
    AliasName="FULLNAME"/>
  <ViewAttribute
    Name="IsGroup"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="IsGroup"
    EntityUsage="Kassa"
    AliasName="IS_GROUP"/>
  <ViewAttribute
    Name="ParentId"
    PrecisionRule="true"
    EntityAttrName="ParentId"
    EntityUsage="Kassa"
    AliasName="PARENT_ID"/>
  <ViewAttribute
    Name="CurrId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="CurrId"
    EntityUsage="Kassa"
    AliasName="CURR_ID"
    LOVName="LOV_CurrId">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="DivisionId"    
    PrecisionRule="true"
    EntityAttrName="DivisionId"
    EntityUsage="Kassa"
    AliasName="DIVISION_ID"
    LOVName="LOV_DivisionId">
    <Properties>
      <SchemaBasedProperties>
        <CONTROLTYPE
          Value="choice"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="FirmaId"
    PrecisionRule="true"
    EntityAttrName="FirmaId"
    EntityUsage="Kassa"
    AliasName="FIRMA_ID"/>
  <ViewAttribute
    Name="Deleted"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Deleted"
    EntityUsage="Kassa"
    AliasName="DELETED"/>
  <ViewAttribute
    Name="Version"
    PrecisionRule="true"
    EntityAttrName="Version"
    EntityUsage="Kassa"
    AliasName="VERSION"/>
  <ViewAttribute
    Name="Predefined"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Predefined"
    EntityUsage="Kassa"
    AliasName="PREDEFINED"/>
  <ViewAttribute
    Name="KEditable"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="K_EDITABLE"
    Expression="K_EDITABLE"
    SQLType="NUMERIC"/>
  <ViewAttribute
    Name="TotalSumm"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[VwKassaMoves.sum("Summ != null ? Summ : 0")]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="TotalPrihod"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[VwKassaMoves.sum("SummPrihod != null ? SummPrihod : 0")]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="TotalRashod"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="VIEW_ATTR"
    SQLType="NUMERIC">
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
    <TransientExpression><![CDATA[VwKassaMoves.sum("SummRashod != null ? SummRashod : 0")]]></TransientExpression>
  </ViewAttribute>
  <ViewLinkAccessor
    Name="KassaView"
    ViewLink="ua.divas.view.KassaKassaFk1Link"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <ViewLinkAccessor
    Name="OrdersView"
    ViewLink="ua.divas.view.OrdersKassaFk1Link"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <ViewLinkAccessor
    Name="UserSettingsView"
    ViewLink="ua.divas.view.UserSettingsKassaFk1Link"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <ViewLinkAccessor
    Name="OrderZamerView"
    ViewLink="ua.divas.view.OrderZamerKassaFkLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <ViewLinkAccessor
    Name="VwKassaMoves"
    ViewLink="ua.divas.view.VwKassaKassaFkLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
</ViewObject>
