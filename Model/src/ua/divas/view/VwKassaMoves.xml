  <SQLQuery><![CDATA[SELECT 
    MOVE_KASSA.ID, 
    MOVE_KASSA.KASSA_ID KASSA_ID, 
    MOVE_KASSA.KASSA KASSA,
    MOVE_KASSA.DIVISION_ID DIVISION_ID,
    MOVE_KASSA.CODE CODE, 
    MOVE_KASSA.SUMM, 
    CASE 
      WHEN MOVE_KASSA.SUMM >= 0 THEN MOVE_KASSA.SUMM 
    END AS summ_prihod, 
    CASE 
      WHEN MOVE_KASSA.SUMM < 0 THEN -1 * MOVE_KASSA.SUMM 
    END AS summ_rashod, 
    MOVE_KASSA.REGISTRATOR_ID REGISTRATOR_ID, 
    MOVE_KASSA.PERIOD PERIOD,
    to_char(MOVE_KASSA.PERIOD,'DD-MM-YYYY') PERIOD_NAME,
    MOVE_KASSA.TABLE_NAME TABLE_NAME,
    CASE
        WHEN upper(MOVE_KASSA.TABLE_NAME) LIKE upper('orders')
            THEN 'Заказ №'||(select num from orders where id = MOVE_KASSA.REGISTRATOR_ID)||' от '||to_char(
            (select dat from orders where id = MOVE_KASSA.REGISTRATOR_ID),'DD-MM-YYYY')
        WHEN upper(MOVE_KASSA.TABLE_NAME) LIKE upper('pko')
            THEN 'ПКО №'||(select num from pko where id = MOVE_KASSA.REGISTRATOR_ID)||' от '||to_char(
            (select dat from pko where id = MOVE_KASSA.REGISTRATOR_ID),'DD-MM-YYYY')  
        WHEN upper(MOVE_KASSA.TABLE_NAME) LIKE upper('rko')
            THEN 'РКО №'||(select num from rko where id = MOVE_KASSA.REGISTRATOR_ID)||' от '||to_char(
            (select dat from rko where id = MOVE_KASSA.REGISTRATOR_ID),'DD-MM-YYYY')  
        WHEN upper(MOVE_KASSA.TABLE_NAME) LIKE upper('other_zatraty')
            THEN 'Админ. затраты №'||(select num from other_zatraty where id = MOVE_KASSA.REGISTRATOR_ID)||' от '||to_char(
            (select dat from other_zatraty where id = MOVE_KASSA.REGISTRATOR_ID),'DD-MM-YYYY')     
        WHEN upper(MOVE_KASSA.TABLE_NAME) LIKE upper('start_ost')
            THEN 'Нач. остатки №'||(select num from start_ost where id = MOVE_KASSA.REGISTRATOR_ID)||' от '||to_char(
            (select dat from start_ost where id = MOVE_KASSA.REGISTRATOR_ID),'DD-MM-YYYY')         
    END AS REG_STRING        
FROM 
    MOVE_KASSA
where
upper(MOVE_KASSA.DIVISION_ID) in (select upper(N) from table(cast(get_divisions(:u_name) as divisionTable)))
ORDER BY PERIOD DESC]]></SQLQuery>
