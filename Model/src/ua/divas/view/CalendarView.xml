<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CalendarView"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  ComponentClass="ua.divas.view.CalendarViewImpl">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Coll|VarAccess"/>
  </DesignTime>
  <Variable
    Name="EndDat"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="Tz"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[TimeZone.getDefault().getID()]]></TransientExpression>
  </Variable>
  <Variable
    Name="StartDat"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <SQLQuery><![CDATA[SELECT Calendar.ID, 
       Calendar.VERSION, 
       Calendar.DAT, 
       Calendar.DESCRIPTION, 
       Calendar.USER_ID, 
       Calendar.CONTACT, 
       Calendar.SERVER, 
       Calendar.CLIENT, 
       Calendar.TRG_NAME, 
       Calendar.USER_LOGIN, 
       Calendar.OBJECT_ID, 
       Calendar.START_DATE, 
       Calendar.END_DATE
FROM  NOTIFICATION Calendar
where Calendar.START_DATE >= :StartDat
and Calendar.END_DATE <= :EndDat
and :Tz is not null]]></SQLQuery>
  <EntityUsage
    Name="Calendar"
    Entity="ua.divas.model.Calendar"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="Calendar"
    AliasName="ID">
    <TransientExpression><![CDATA[(Id==null?UUID.randomUUID().toString():Id)]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="Version"
    PrecisionRule="true"
    EntityAttrName="Version"
    EntityUsage="Calendar"
    AliasName="VERSION"/>
  <ViewAttribute
    Name="Dat"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Dat"
    EntityUsage="Calendar"
    AliasName="DAT">
    <TransientExpression><![CDATA[adf.currentDateTime]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="Description"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Description"
    EntityUsage="Calendar"
    AliasName="DESCRIPTION"/>
  <ViewAttribute
    Name="UserId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="UserId"
    EntityUsage="Calendar"
    AliasName="USER_ID">
    <TransientExpression><![CDATA[viewObject.getSessionUserId()]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="Contact"
    PrecisionRule="true"
    EntityAttrName="Contact"
    EntityUsage="Calendar"
    AliasName="CONTACT"/>
  <ViewAttribute
    Name="Server"
    PrecisionRule="true"
    EntityAttrName="Server"
    EntityUsage="Calendar"
    AliasName="SERVER"
    DefaultValue="0"/>
  <ViewAttribute
    Name="Client"
    PrecisionRule="true"
    EntityAttrName="Client"
    EntityUsage="Calendar"
    AliasName="CLIENT"
    DefaultValue="0"/>
  <ViewAttribute
    Name="TrgName"
    PrecisionRule="true"
    EntityAttrName="TrgName"
    EntityUsage="Calendar"
    AliasName="TRG_NAME"/>
  <ViewAttribute
    Name="UserLogin"
    PrecisionRule="true"
    EntityAttrName="UserLogin"
    EntityUsage="Calendar"
    AliasName="USER_LOGIN">
    <TransientExpression><![CDATA[viewObject.getSessionUser()]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="ObjectId"
    PrecisionRule="true"
    EntityAttrName="ObjectId"
    EntityUsage="Calendar"
    AliasName="OBJECT_ID"/>
  <ViewAttribute
    Name="StartDate"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="StartDate"
    EntityUsage="Calendar"
    AliasName="START_DATE">
    <TransientExpression><![CDATA[adf.currentDateTime]]></TransientExpression>
  </ViewAttribute>
  <ViewAttribute
    Name="EndDate"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="EndDate"
    EntityUsage="Calendar"
    AliasName="END_DATE">
    <TransientExpression><![CDATA[adf.currentDate+1]]></TransientExpression>
  </ViewAttribute>
</ViewObject>
