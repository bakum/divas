    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT MOVE_KONTRAG.ID,
  MOVE_KONTRAG.KONTRAG_ID,
  MOVE_KONTRAG.KONTRAG,
  MOVE_KONTRAG.DIVISION_ID DIVISION_ID,
  MOVE_KONTRAG.TABLE_NAME TABLE_NAME,  
  MOVE_KONTRAG.CODE,
  MOVE_KONTRAG.SUMM,
  case 
        when MOVE_KONTRAG.SUMM >=0
        then MOVE_KONTRAG.SUMM
    end as summ_rashod,
    case 
        when MOVE_KONTRAG.SUMM < 0
        then (-1)*MOVE_KONTRAG.SUMM
    end as summ_prihod,
  MOVE_KONTRAG.REGISTRATOR_ID,
  MOVE_KONTRAG.PERIOD,
  KONTRAGENTS.IS_MEASURER,
  CASE
        WHEN upper(MOVE_KONTRAG.TABLE_NAME) LIKE upper('orders')
            THEN 'Заказ №'||(select num from orders where id = MOVE_KONTRAG.REGISTRATOR_ID)||' от '||to_char(
            (select dat from orders where id = MOVE_KONTRAG.REGISTRATOR_ID),'DD-MM-YYYY')
        WHEN upper(MOVE_KONTRAG.TABLE_NAME) LIKE upper('pko')
            THEN 'ПКО №'||(select num from pko where id = MOVE_KONTRAG.REGISTRATOR_ID)||' от '||to_char(
            (select dat from pko where id = MOVE_KONTRAG.REGISTRATOR_ID),'DD-MM-YYYY')  
        WHEN upper(MOVE_KONTRAG.TABLE_NAME) LIKE upper('rko')
            THEN 'РКО №'||(select num from rko where id = MOVE_KONTRAG.REGISTRATOR_ID)||' от '||to_char(
            (select dat from rko where id = MOVE_KONTRAG.REGISTRATOR_ID),'DD-MM-YYYY')  
        WHEN upper(MOVE_KONTRAG.TABLE_NAME) LIKE upper('other_zatraty')
            THEN 'Админ. затраты №'||(select num from other_zatraty where id = MOVE_KONTRAG.REGISTRATOR_ID)||' от '||to_char(
            (select dat from other_zatraty where id = MOVE_KONTRAG.REGISTRATOR_ID),'DD-MM-YYYY')     
    END AS REG_STRING  
FROM MOVE_KONTRAG
INNER JOIN KONTRAGENTS
ON MOVE_KONTRAG.KONTRAG_ID    = KONTRAGENTS.ID
WHERE KONTRAGENTS.IS_MEASURER = 1
and
upper(MOVE_KONTRAG.DIVISION_ID) in (select upper(N) from table(cast(get_divisions(:u_name) as divisionTable)))]]></SQLQuery>
