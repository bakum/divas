  <SQLQuery><![CDATA[SELECT 
    RKO.ID ID, 
    RKO.DAT DAT, 
    RKO.NUM NUM, 
    RKO.DELETED DELETED, 
    RKO.POSTED POSTED, 
    RKO.DIVISION_ID DIVISION_ID, 
    RKO.USER_ID USER_ID, 
    RKO.COMMENTS COMMENTS, 
    RKO.VERSION VERSION, 
    RKO.CURR_ID CURR_ID, 
    RKO.KASSA_ID KASSA_ID, 
    RKO.ACTIVITIES_ID ACTIVITIES_ID, 
    RKO.OPERATION_ID OPERATION_ID, 
    RKO.KONTRAG_ID KONTRAG_ID,
    case
        when
        RKO.SUMMA >=0 then
        RKO.SUMMA else 0
    end SUMMA, 
    case
        when
        RKO.SUMMA < 0 then
        -RKO.SUMMA else 0
    end SUMMA_RASH, 
    RKO.DEST_KASSA_ID DEST_KASSA_ID, 
    RKO.ORDER_ID ORDER_ID,
    Reg_String
from (    
SELECT 
    RKO.ID ID, 
    RKO.DAT DAT, 
    RKO.NUM NUM, 
    RKO.DELETED DELETED, 
    RKO.POSTED POSTED, 
    RKO.DIVISION_ID DIVISION_ID, 
    RKO.USER_ID USER_ID, 
    RKO.COMMENTS COMMENTS, 
    RKO.VERSION VERSION, 
    RKO.CURR_ID CURR_ID, 
    RKO.KASSA_ID KASSA_ID, 
    RKO.ACTIVITIES_ID ACTIVITIES_ID, 
    RKO.OPERATION_ID OPERATION_ID, 
    RKO.KONTRAG_ID KONTRAG_ID, 
    RKO.SUMMA SUMMA, 
    RKO.DEST_KASSA_ID DEST_KASSA_ID, 
    RKO.ORDER_ID ORDER_ID,
    'РКО №'||Rko.NUM|| ' от '|| TO_CHAR(
        rko.DAT, 'DD-MM-YYYY') Reg_String
FROM 
    RKO
WHERE RKO.POSTED = 1
union
SELECT 
    PKO.ID ID, 
    PKO.DAT DAT, 
    PKO.NUM NUM, 
    PKO.DELETED DELETED, 
    PKO.POSTED POSTED, 
    PKO.DIVISION_ID DIVISION_ID, 
    PKO.USER_ID USER_ID, 
    PKO.COMMENTS COMMENTS, 
    PKO.VERSION VERSION, 
    PKO.CURR_ID CURR_ID, 
    PKO.KASSA_ID KASSA_ID, 
    PKO.ACTIVITIES_ID ACTIVITIES_ID, 
    PKO.OPERATION_ID OPERATION_ID, 
    PKO.KONTRAG_ID KONTRAG_ID, 
    -PKO.SUMMA SUMMA, 
    NULL DEST_KASSA_ID, 
    PKO.ORDER_ID ORDER_ID,
    'ПКО №'||PKO.NUM|| ' от '|| TO_CHAR(
        PKO.DAT, 'DD-MM-YYYY') Reg_String
FROM 
    PKO
WHERE PKO.POSTED = 1) RKO]]></SQLQuery>
