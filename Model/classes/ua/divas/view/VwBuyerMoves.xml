  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
    <Attr Name="_codeGenFlag2" Value="Coll|VarAccess"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="ua.divas.view.VwBuyerMoves_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="u_name"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[viewObject.getSessionUser()]]></TransientExpression>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT MOVE_KONTRAG.ID,
  MOVE_KONTRAG.KONTRAG_ID,
  MOVE_KONTRAG.KONTRAG,
  MOVE_KONTRAG.DIVISION_ID DIVISION_ID,
  MOVE_KONTRAG.CODE,
  MOVE_KONTRAG.SUMM,
  case 
        when MOVE_KONTRAG.SUMM >=0
        then MOVE_KONTRAG.SUMM
    end as summ_prihod,
    case 
        when MOVE_KONTRAG.SUMM < 0
        then (-1)*MOVE_KONTRAG.SUMM
    end as summ_rashod,
  MOVE_KONTRAG.REGISTRATOR_ID,
  MOVE_KONTRAG.PERIOD
FROM MOVE_KONTRAG
INNER JOIN KONTRAGENTS
ON MOVE_KONTRAG.KONTRAG_ID    = KONTRAGENTS.ID
WHERE KONTRAGENTS.IS_BUYER = 1 and MOVE_KONTRAG.CODE='20711'
and
upper(MOVE_KONTRAG.DIVISION_ID) in (select upper(N) from table(cast(get_divisions(:u_name) as divisionTable)))]]></SQLQuery>
