  <SQLQuery><![CDATA[SELECT Orders.ACTIVITIES_ID, 
       Orders.CURR_ID, 
       Orders.DAT, 
       Orders.DELETED, 
       Orders.DISCRIPTION, 
       Orders.DIVISION_ID, 
       Orders.FIRM_ID, 
       Orders.ID, 
       Orders.KASSA_ID, 
       Orders.KONTRAG_ID, 
       (select fullname from kontragents where id = KONTRAG_ID and rownum = 1) KontragName,
       Orders.KRATNOST, 
       Orders.KURS, 
       Orders.NUM, 
       Orders.POSTED, 
       Orders.STATUS_ID, 
       (select 0 from dual) o_editable,
       Orders.USER_ID, 
       Orders.VERSION, 
       Orders.ZAMER_ID,
       (select fullname from kontragents where id = ZAMER_ID and rownum = 1) ZamerName,
       (select adress from contact_details where kontrag_id = Orders.KONTRAG_ID and rownum = 1) adress
FROM  ORDERS Orders
where deleted = 0 and
status_id = (select id from order_status where upper(name)=upper('Замер')
and rownum = 1)]]></SQLQuery>
