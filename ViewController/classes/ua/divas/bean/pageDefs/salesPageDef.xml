<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.2.66.68" id="salesPageDef"
                Package="ua.divas.bean.pageDefs">
  <parameters/>
  <executables>
    <iterator Binds="VwSales1" RangeSize="25" DataControl="AppModuleDataControl" id="VwSales1Iterator">
      <sortCriteria>
        <sort attribute="Period" ascending="false"/>
      </sortCriteria>
    </iterator>
    <searchRegion Binds="VwSales1Iterator" Criteria="" Customizer="oracle.jbo.uicli.binding.JUSearchBindingCustomizer"
                  id="VwSales1Query"/>
    <iterator id="DivisionsView1_1Iterator" Binds="DivisionsView1_1" DataControl="AppModuleDataControl" RangeSize="-1">
      <sortCriteria>
        <sort attribute="Fullname" ascending="true"/>
      </sortCriteria>
    </iterator>
    <variableIterator id="variables">
      <variableUsage DataControl="AppModuleDataControl" Binds="VwSales1.variablesMap.f_dat"
                     Name="ExecuteWithParams_f_dat" IsQueriable="false"/>
      <variableUsage DataControl="AppModuleDataControl" Binds="VwSales1.variablesMap.l_dat"
                     Name="ExecuteWithParams_l_dat" IsQueriable="false"/>
    </variableIterator>
  </executables>
  <bindings>
    <tree IterBinding="VwSales1Iterator" id="VwSales1">
      <nodeDefinition DefName="ua.divas.view.VwSales" Name="VwSales10">
        <AttrNames>
          <Item Value="OrderId"/>
          <Item Value="KontragId"/>
          <Item Value="DivisionId"/>
          <Item Value="Period"/>
          <Item Value="SalesSumma"/>
          <Item Value="FactSebestSumma"/>
          <Item Value="PlanSebestSumma"/>
