    </tree>
    <list StaticList="false" IterBinding="VwSales1Iterator" ListIter="DivisionsView1_1Iterator" id="DivisionId"
          NullValueId="${adfBundle['oracle.javatools.resourcebundle.SystemBundle']['NULL_VALUE_RESID']}"
          NullValueFlag="start">
      <AttrNames>
        <Item Value="DivisionId"/>
      </AttrNames>
      <ListAttrNames>
        <Item Value="Id"/>
      </ListAttrNames>
      <ListDisplayAttrNames>
        <Item Value="Fullname"/>
      </ListDisplayAttrNames>
    </list>
    <action IterBinding="VwSales1Iterator" id="ExecuteWithParams" RequiresUpdateModel="true" Action="executeWithParams">
      <NamedData NDName="f_dat" NDType="oracle.jbo.domain.Date" NDValue="${bindings.ExecuteWithParams_f_dat}"/>
      <NamedData NDName="l_dat" NDType="oracle.jbo.domain.Date" NDValue="${bindings.ExecuteWithParams_l_dat}"/>
    </action>
    <attributeValues IterBinding="variables" id="f_dat">
      <AttrNames>
        <Item Value="ExecuteWithParams_f_dat"/>
      </AttrNames>
    </attributeValues>
    <attributeValues IterBinding="variables" id="l_dat">
      <AttrNames>
        <Item Value="ExecuteWithParams_l_dat"/>
      </AttrNames>
    </attributeValues>
  </bindings>
</pageDefinition>
