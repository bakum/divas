                                            <af:selectOneChoice value="#{bindings.CurrId.inputValue}"
                                                                label="#{bindings.CurrId.label}"
                                                                required="#{bindings.CurrId.hints.mandatory}"
                                                                shortDesc="#{bindings.CurrId.hints.tooltip}" id="soc2"
                                                                labelStyle="font-weight: bold;"
                                                                readOnly="#{bindings.OEditable.inputValue == 1 ? false : true}">
                                                <f:selectItems value="#{bindings.CurrId.items}" id="si2"/>
                                                <f:validator binding="#{bindings.CurrId.validator}"/>
                                            </af:selectOneChoice>
                                            <af:selectOneChoice value="#{bindings.KassaId.inputValue}"
                                                                label="#{bindings.KassaId.label}"
                                                                required="#{bindings.KassaId.hints.mandatory}"
                                                                shortDesc="#{bindings.KassaId.hints.tooltip}" id="soc3"
                                                                labelStyle="font-weight: bold;"
                                                                readOnly="#{bindings.OEditable.inputValue == 1 ? false : true}">
                                                <f:selectItems value="#{bindings.KassaId.items}" id="si3"/>
                                                <f:validator binding="#{bindings.KassaId.validator}"/>
                                            </af:selectOneChoice>
                                        </af:panelGroupLayout>
                                    </af:panelLabelAndMessage>
