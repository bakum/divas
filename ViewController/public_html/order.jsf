                                            <af:selectOneChoice value="#{bindings.FirmId.inputValue}"
                                                                label="#{bindings.FirmId.label}"
                                                                required="#{bindings.FirmId.hints.mandatory}"
                                                                shortDesc="#{bindings.FirmId.hints.tooltip}" id="soc1"
                                                                labelStyle="font-weight: bold;"
                                                                readOnly="#{bindings.OEditable.inputValue == 1 ? false : true}">
