                                                  label="#{bindings.Kurs.hints.label}"
                                                  required="#{bindings.Kurs.hints.mandatory}"
                                                  columns="#{bindings.Kurs.hints.displayWidth}"
                                                  maximumLength="#{bindings.Kurs.hints.precision}"
                                                  shortDesc="#{bindings.Kurs.hints.tooltip}" id="it2" visible="false">
                                        <f:validator binding="#{bindings.Kurs.validator}"/>
                                        <af:convertNumber groupingUsed="false" pattern="#{bindings.Kurs.format}"/>
                                    </af:inputText>
                                    <af:inputText value="#{bindings.Kratnost.inputValue}"
                                                  label="#{bindings.Kratnost.hints.label}"
                                                  required="#{bindings.Kratnost.hints.mandatory}"
                                                  columns="#{bindings.Kratnost.hints.displayWidth}"
                                                  maximumLength="#{bindings.Kratnost.hints.precision}"
                                                  shortDesc="#{bindings.Kratnost.hints.tooltip}" id="it3"
                                                  visible="false">
                                        <f:validator binding="#{bindings.Kratnost.validator}"/>
                                        <af:convertNumber groupingUsed="false" pattern="#{bindings.Kratnost.format}"/>
                                    </af:inputText>
                                    <af:inputText value="#{bindings.Discription.inputValue}"
                                                  label="#{bindings.Discription.hints.label}"
                                                  required="#{bindings.Discription.hints.mandatory}"
                                                  columns="#{bindings.Discription.hints.displayWidth}"
                                                  maximumLength="#{bindings.Discription.hints.precision}"
                                                  shortDesc="#{bindings.Discription.hints.tooltip}" id="it4" rows="4">
                                        <f:validator binding="#{bindings.Discription.validator}"/>
                                    </af:inputText>
                                    <af:separator id="s3"/>
                                    <af:panelLabelAndMessage id="plam5">
                                        <af:panelGroupLayout id="pgl7" layout="horizontal">
                                            <af:selectOneChoice value="#{bindings.ActivitiesId.inputValue}"
                                                                label="#{bindings.ActivitiesId.label}"
                                                                required="#{bindings.ActivitiesId.hints.mandatory}"
                                                                shortDesc="#{bindings.ActivitiesId.hints.tooltip}"
                                                                id="soc6" labelStyle="font-weight: bold;"
                                                                readOnly="#{bindings.OEditable.inputValue == 1 ? false : true}">
                                                <f:selectItems value="#{bindings.ActivitiesId.items}" id="si6"/>
