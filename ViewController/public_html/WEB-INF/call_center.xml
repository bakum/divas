<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="call_center">
    <default-activity>ccenter</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>filter</managed-bean-name>
      <managed-bean-class>ua.divas.view.FilterBeans</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>AutoSuggestonKontragents</managed-bean-name>
      <managed-bean-class>ua.divas.view.AutoSuggestonKontragents</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="ccenter">
      <page>/ccenter.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
