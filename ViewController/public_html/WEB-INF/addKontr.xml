<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="addKontr">
    <default-activity>CreateInsert</default-activity>
    <transaction>
      <requires-transaction/>
    </transaction>
    <return-value-definition id="__1">
      <name>KontragId</name>
      <value>#{pageFlowScope.KontragId}</value>
    </return-value-definition>
    <view id="addKontragent">
      <page>/addKontragent.jsf</page>
      <train-stop/>
    </view>
    <task-flow-return id="ok">
      <outcome>
        <name>ok</name>
        <commit/>
      </outcome>
    </task-flow-return>
    <task-flow-return id="cancel">
      <outcome>
        <name>cancel</name>
        <rollback/>
      </outcome>
    </task-flow-return>
    <method-call id="CreateInsert">
      <method>#{bindings.CreateInsert.execute}</method>
      <outcome>
        <fixed-outcome>CreateInsert</fixed-outcome>
      </outcome>
    </method-call>
    <train/>
    <control-flow-rule id="__2">
      <from-activity-id>addKontragent</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>ok</from-outcome>
        <to-activity-id>ok</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__4">
        <from-outcome>cancel</from-outcome>
        <to-activity-id>cancel</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__5">
      <from-activity-id>CreateInsert</from-activity-id>
      <control-flow-case id="__6">
        <from-outcome>CreateInsert</from-outcome>
        <to-activity-id>addKontragent</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-config>
