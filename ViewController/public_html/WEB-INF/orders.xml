<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="orders">
    <default-activity>order</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>orders</managed-bean-name>
      <managed-bean-class>ua.divas.bean.OrdersBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="order">
      <page>/orders.jsff</page>
    </view>
    <task-flow-call id="create-kontragent">
      <task-flow-reference>
        <document>/WEB-INF/create-kontragent.xml</document>
        <id>create-kontragent</id>
      </task-flow-reference>
    </task-flow-call>
    <control-flow-rule id="__6">
      <from-activity-id>order</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>addOrder</from-outcome>
        <to-activity-id>create-kontragent</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
