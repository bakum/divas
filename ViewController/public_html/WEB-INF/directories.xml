<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="direct">
    <default-activity>director</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>currency</managed-bean-name>
      <managed-bean-class>ua.divas.view.CurrencyBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>firms</managed-bean-name>
      <managed-bean-class>ua.divas.view.FirmsBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <view id="director">
      <page>/director.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
